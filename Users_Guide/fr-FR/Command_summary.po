#. Tag: title
#, no-c-format
msgid "Command summary"
msgstr "Résumé des commandes"

#. Tag: title
#, no-c-format
msgid "Command options"
msgstr "Options des commandes"

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican --help</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "displays help"
msgstr "affiche une aide"

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican --man</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "displays the manual page"
msgstr "affiche la page du manuel"

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican --help_actions</command>"
msgstr "<command>publican --help_actions</command>"

#. Tag: para
#, no-c-format
msgid "displays a list of <firstterm>actions</firstterm>"
msgstr "affiche la liste des <firstterm>actions</firstterm>"

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican --v</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "displays the <application>Publican</application> version number."
msgstr "affiche le numéro de version de <application>Publican</application>"

#. Tag: term
#, no-c-format
msgid "<option>--config <replaceable>file</replaceable></option>"
msgstr "<option>--config <replaceable>fichier</replaceable></option>"

#. Tag: para
#, no-c-format
msgid "specifies a config file for a document, in place of the default <filename>publican.cfg</filename>."
msgstr "définit un fichier de configuration pour un document, à la place de <filename>publican.cfg</filename> par défaut."

#. Tag: term
#, no-c-format
msgid "<option>--nocolours</option>"
msgstr "<option>--nocolours</option>"

#. Tag: para
#, no-c-format
msgid "disables ANSI colors in <application>Publican</application> logging."
msgstr "désactive les couleurs ANSI dans la journalisation de <application>Publican</application>."

#. Tag: term
#, no-c-format
msgid "<option>--quiet</option>"
msgstr "<option>--quiet</option>"

#. Tag: para
#, no-c-format
msgid "disables all logging."
msgstr "désactive toute journalisation."

#. Tag: title
#, no-c-format
msgid "Actions"
msgstr "Actions"

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican add_revision</command>"
msgstr "<command>publican add_revision</command>"

#. Tag: para
#, no-c-format
msgid "adds an entry in <filename>Revision_History.xml</filename>. Options:"
msgstr "ajoute une entrée dans <filename>Revision_History.xml</filename>. Options :"

#. Tag: term
#, no-c-format
msgid "--lang=<replaceable>LANG</replaceable>"
msgstr "--lang=<replaceable>LANGUE</replaceable>"

#. Tag: para
#, no-c-format
msgid "the language the XML will be written in."
msgstr "la langue dans laquelle est écrit le XML."

#. Tag: term
#, no-c-format
msgid "--revnumber=<replaceable>REVNUMBER</replaceable>"
msgstr "--revnumber=<replaceable>NUMÉRO_RÉVISION</replaceable>"

#. Tag: para
#, no-c-format
msgid "revision number to use for a revision."
msgstr "numéro de révision à utiliser pour une révision."

#. Tag: term
#, no-c-format
msgid "--date=<replaceable>DATE</replaceable>"
msgstr "--date=<replaceable>DATE</replaceable>"

#. Tag: para
#, no-c-format
msgid "date to use for a revision."
msgstr "date à utiliser pour une révision."

#. Tag: term
#, no-c-format
msgid "--member=<replaceable>MEMBER</replaceable>"
msgstr "--member=<replaceable>MEMBRE</replaceable>"

#. Tag: para
#, no-c-format
msgid "an entry to be added to the revision. Can be specified multiple times."
msgstr "une entrée à ajouter à la révision. Peut être précisée plusieurs fois."

#. Tag: term
#, no-c-format
msgid "--firstname=<replaceable>FIRSTNAME</replaceable>"
msgstr "--firstname=<replaceable>PRÉNOM</replaceable>"

#. Tag: para
#, no-c-format
msgid "firstname to use for a revision."
msgstr "prénom à utiliser pour une révision"

#. Tag: term
#, no-c-format
msgid "--surname=<replaceable>SURNAME</replaceable>"
msgstr "--surname=<replaceable>NOM_FAMILLE</replaceable>"

#. Tag: para
#, no-c-format
msgid "surname to use for a revision."
msgstr "nom de famille à utiliser pour la révision."

#. Tag: term
#, no-c-format
msgid "--email=<replaceable>EMAIL</replaceable>"
msgstr "--email=<replaceable>E-MAIL</replaceable>"

#. Tag: para
#, no-c-format
msgid "email to use for a revision."
msgstr "adresse électronique à utiliser pour la révision."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican build</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "transforms XML into a document. Options:"
msgstr "transforme un XML en document. Options :"

#. Tag: term
#, no-c-format
msgid "--help"
msgstr "--help"

#. Tag: para
#, no-c-format
msgid "display help message."
msgstr "affiche un message d'aide."

#. Tag: term
#, no-c-format
msgid "--config=s"
msgstr "--config=s"

#. Tag: para
#, no-c-format
msgid "use a nonstandard config file."
msgstr "utilise un fichier de configuration non standard."

#. Tag: term
#, no-c-format
msgid "--common_config=s"
msgstr "--common_config=s"

#. Tag: para
#, no-c-format
msgid "override path to Common_Config directory."
msgstr "chemin se substituant à celui vers le répertoire Common_Config."

#. Tag: term
#, no-c-format
msgid "--common_content=s"
msgstr "--common_content=s"

#. Tag: para
#, no-c-format
msgid "override path to Common_Content directory."
msgstr "chemin se substituant à celui vers le répertoire Common_Config."

#. Tag: term
#, no-c-format
msgid "--nocolours"
msgstr "--nocolours"

#. Tag: para
#, no-c-format
msgid "disable ANSI colorization of logging."
msgstr "désactive les couleurs ANSI de la journalisation."

#. Tag: term
#, no-c-format
msgid "--quiet"
msgstr "--quiet"

#. Tag: para
#, no-c-format
msgid "disable all logging."
msgstr "désactive toute journalisation."

#. Tag: term
#, no-c-format
msgid "--brand_dir=s"
msgstr "--brand_dir=s"

#. Tag: para
#, no-c-format
msgid "directory to source brand files from."
msgstr "répertoire où rechercher les fichiers de la marque."

#. Tag: term
#, no-c-format
msgid "--formats=<replaceable>FORMATS</replaceable>"
msgstr "--formats=<replaceable>FORMATS</replaceable>"

#. Tag: para
#, no-c-format
msgid "comma-separated list of formats to build. For example: html,pdf,html-single,html-desktop,txt,epub (mandatory)."
msgstr "liste, avec une virgule comme séparateur, des formats à construire. Par exemple : html,pdf,html-single,html-desktop,txt,epub (obligatoire)."

#. Tag: term
#, no-c-format
msgid "--langs=<replaceable>LANGS</replaceable>"
msgstr "--langs=<replaceable>LANGUES</replaceable>"

#. Tag: para
#, no-c-format
msgid "comma-separated list of languages to build. For example: en-US,de-DE,all (mandatory)."
msgstr "liste, avec une virgule comme séparateur, des langues à construire. Par exemple : en-US,de-DE,all (obligatoire)."

#. Tag: term
#, no-c-format
msgid "--publish"
msgstr "--publish"

#. Tag: para
#, no-c-format
msgid "sets up built content for publishing."
msgstr "définit le type de construction d'un contenu de publication."

#. Tag: term
#, no-c-format
msgid "--embedtoc"
msgstr "--embedtoc"

#. Tag: para
#, no-c-format
msgid "embeds a table of contents into HTML output."
msgstr "intègre une table des matières dans la sortie HTML."

#. Tag: term
#, no-c-format
msgid "--novalid"
msgstr "--novalid"

#. Tag: para
#, no-c-format
msgid "skips DTD validation when building a document."
msgstr "n'effectue pas la validation par rapport au DTD lors de la construction du document."

#. Tag: term
#, no-c-format
msgid "--src_dir=<replaceable>SRC_DIR</replaceable>"
msgstr "--src_dir=<replaceable>RÉPERTOIRE_SOURCE</replaceable>"

#. Tag: para
#, no-c-format
msgid "specifies the directory to source <application>Publican</application> files from."
msgstr "définit le répertoire dans lequel retrouver les fichiers sources pour <application>Publican</application>."

#. Tag: term
#, no-c-format
msgid "--pdftool=s"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Override the tool to use when creating PDFs. Valid options are wkhtmltopdf and fop."
msgstr ""

#. Tag: term
#, no-c-format
msgid "--src_dir=<replaceable>SRC_DIR</replaceable>"
msgstr "--src_dir=<replaceable>RÉPERTOIRE_SOURCE</replaceable>"

#. Tag: para
#, no-c-format
msgid "Override the directory to publish files to. Defaults to 'publish'."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican clean</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "removes the temporary directories from a document directory."
msgstr "supprime les répertoires temporaires dans un répertoire de document."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican clean_ids</command>"
msgstr "<command>publican clean_ids</command>"

#. Tag: para
#, no-c-format
msgid "indents XML files neatly, and rebuilds element IDs."
msgstr "indente les fichiers XML proprement et reconstruit les identifiants des éléments."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican clean_set</command>"
msgstr "<command>publican clean_set</command>"

#. Tag: para
#, no-c-format
msgid "removes local copies of remote books that are part of a set."
msgstr "supprime les copies locales des ouvrages distants faisant partie d'un ensemble."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican create</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "creates a new book, article, or set. Options:"
msgstr "crée des nouveaux ouvrage, article ou ensemble. Options :"

#. Tag: term
#, no-c-format
msgid "--name"
msgstr "--name"

#. Tag: para
#, no-c-format
msgid "the name of the document (mandatory)."
msgstr "le nom du document (obligatoire)."

#. Tag: term
#, no-c-format
msgid "--product"
msgstr "--product"

#. Tag: para
#, no-c-format
msgid "the documented product."
msgstr "le produit documenté."

#. Tag: term
#, no-c-format
msgid "--version"
msgstr "--version"

#. Tag: para
#, no-c-format
msgid "the version of the documented product."
msgstr "la version du produit documenté."

#. Tag: term
#, no-c-format
msgid "--edition"
msgstr "--edition"

#. Tag: para
#, no-c-format
msgid "the edition of the document."
msgstr "l'édition du document."

#. Tag: term
#, no-c-format
msgid "--brand"
msgstr "--brand"

#. Tag: para
#, no-c-format
msgid "the brand for the document."
msgstr "la charte graphique du document (marque de fabrique)."

#. Tag: term
#, no-c-format
msgid "--lang"
msgstr "--lang"

#. Tag: para
#, no-c-format
msgid "the language in which the XML will be authored."
msgstr "la langue dans laquelle le XML a été écrit."

#. Tag: term
#, no-c-format
msgid "--type"
msgstr "--type"

#. Tag: para
#, no-c-format
msgid "the type of document — article, book, or set."
msgstr "le type de document — article, livre ou ensemble."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican create_brand</command>"
msgstr "<command>publican create_brand</command>"

#. Tag: para
#, no-c-format
msgid "creates a new brand. Options:"
msgstr "crée une nouvelle marque de fabrique. Options :"

#. Tag: term
#, no-c-format
msgid "publican create_site"
msgstr "publican create_site"

#. Tag: para
#, no-c-format
msgid "creates a documentation website. Options:"
msgstr "crée un site Web de documentation. Options :"

#. Tag: term
#, no-c-format
msgid "--site_config"
msgstr "--site_config"

#. Tag: para
#, no-c-format
msgid "name of the site configuration file to create (mandatory)."
msgstr "nom du fichier de configuration du site à créer (obligatoire)."

#. Tag: term
#, no-c-format
msgid "--db_file"
msgstr "--db_file"

#. Tag: para
#, no-c-format
msgid "name of the site database file to create (mandatory)."
msgstr "nom du fichier de base de données du site à créer (obligatoire)."

#. Tag: term
#, no-c-format
msgid "--toc_path"
msgstr "--toc_path"

#. Tag: para
#, no-c-format
msgid "path to the directory in which to create the top-level <filename>toc.html</filename> file (mandatory)."
msgstr "chemin vers le répertoire où créer le fichier <filename>toc.html</filename> de haut niveau (obligatoire)."

#. Tag: term
#, no-c-format
msgid "--tmpl_path"
msgstr "--tmpl_path"

#. Tag: para
#, no-c-format
msgid "path to the template directory (by default, <filename>/usr/share/publican/templates</filename>)."
msgstr "chemin vers le répertoire du modèle (par défaut, <filename>/usr/share/publican/templates</filename>)."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican help_config</command>"
msgstr "<command>publican help_config</command>"

#. Tag: para
#, no-c-format
msgid "displays a list of parameters for the <filename>publican.cfg</filename> file."
msgstr "affiche la liste des paramètres pour le fichier <filename>publican.cfg</filename>."

#. Tag: term
#, no-c-format
msgid "publican install_book"
msgstr "publican install_book"

#. Tag: para
#, no-c-format
msgid "installs a document on a documentation website."
msgstr "installe un document sur un site Web de documentation."

#. Tag: para
#, no-c-format
msgid "name of the site configuration file (mandatory)."
msgstr "nom du fichier de configuration du site (obligatoire)."

#. Tag: para
#, no-c-format
msgid "the language of the document to install (mandatory)."
msgstr "la langue du document à installer (obligatoire)."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican install_brand</command>"
msgstr "<command>publican install_brand</command>"

#. Tag: para
#, no-c-format
msgid "configures a brand for installation. Option:"
msgstr "configure une marque en vue de son installation. Option :"

#. Tag: term
#, no-c-format
msgid "--path"
msgstr "--path"

#. Tag: para
#, no-c-format
msgid "path to the <application>Publican</application> Common Content files. By default, <filename>/usr/share/publican/Common_Content</filename> on Linux operating systems and at <filename>%SystemDrive%/%ProgramFiles%/Publican/Common_Content</filename> on Windows operating systems — typically, <filename>C:/Program Files/Publican/Common_Content</filename>"
msgstr "chemin vers les fichiers « Common Content » de <application>Publican</application>. Par défaut, <filename>/usr/share/publican/Common_Content</filename> sur les systèmes d'exploitation Linux et <filename>%SystemDrive%/%ProgramFiles%/Publican/Common_Content</filename> sur Windows — généralement, <filename>C:/Program Files/Publican/Common_Content</filename>"

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican copy_web_brand</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "copy a brand's installed web content to another site. Options:"
msgstr ""

#. Tag: para
#, no-c-format
msgid "the brand to copy."
msgstr ""

#. Tag: term
#, no-c-format, fuzzy
msgid "--site_config=<replaceable>site_config</replaceable>"
msgstr "--site_config=<replaceable>configuration_site</replaceable>"

#. Tag: para
#, no-c-format
msgid "website configuration file to copy brand web content from."
msgstr ""

#. Tag: term
#, no-c-format, fuzzy
msgid "--site_config=<replaceable>site_config</replaceable>"
msgstr "--site_config=<replaceable>configuration_site</replaceable>"

#. Tag: para
#, no-c-format
msgid "website configuration file to copy brand web content to."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican lang_stats</command>"
msgstr "<command>publican lang_stats</command>"

#. Tag: para
#, no-c-format
msgid "generates a translation report for a language."
msgstr "génère un rapport sur la traduction pour une langue."

#. Tag: term
#, no-c-format
msgid "--langs"
msgstr "--langs"

#. Tag: para
#, no-c-format
msgid "a comma-separated list of languages for which the report will be generated."
msgstr "une liste, avec une virgule comme séparateur, des langues pour lesquelles le rapport doit être créé."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican migrate_site</command>"
msgstr "<command>publican migrate_site</command>"

#. Tag: para
#, no-c-format
msgid "migrates a website database from <application>Publican</application> 2.x to <application>Publican</application> 3. Options:"
msgstr "opère la migration d'une base de données d'un site Web de <application>Publican</application> 2.x vers <application>Publican</application> 3. Options :"

#. Tag: para
#, no-c-format
msgid "website configuration file to use or create."
msgstr "fichier de configuration du site Web à utiliser ou créer."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican package</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "packages a document or brand for distribution. Options:"
msgstr "empaquette un document ou une marque en vue de sa distribution. Options :"

#. Tag: para
#, no-c-format
msgid "the language to package (mandatory for documents, meaningless for brands)."
msgstr "la langue à empaqueter (obligatoire pour les documents, sans signification pour les marques)."

#. Tag: term
#, no-c-format
msgid "--desktop"
msgstr "--desktop"

#. Tag: para
#, no-c-format
msgid "specifies that a document RPM package should be built for desktop use (meaningless for brands)."
msgstr "précise que le paquet RPM du document doit être construit en vue d'une utilisation de bureau (sans signification pour les marques)."

#. Tag: term
#, no-c-format
msgid "--brew"
msgstr "--brew"

#. Tag: para
#, no-c-format
msgid "pushes a package to the <application>Brew</application> build system (meaningless outside Red Hat)."
msgstr "place le paquet dans le système de construction <application>Brew</application> (sans signification en dehors de Red Hat)."

#. Tag: term
#, no-c-format
msgid "--scratch"
msgstr "--scratch"

#. Tag: para
#, no-c-format
msgid "used in conjunction with <option>--brew</option> to specify a <firstterm>scratch build</firstterm> (meaningless outside Red Hat)."
msgstr "utilisé conjointement avec <option>--brew</option> pour demander une <firstterm>construction à partir de zéro</firstterm> (sans signification en dehors de Red Hat)."

#. Tag: term
#, no-c-format
msgid "--short_sighted"
msgstr "--short_sighted"

#. Tag: para
#, no-c-format
msgid "builds the package without the product version number in the package name."
msgstr "construit le paquet sans numéro de version du produit dans le nom du paquet."

#. Tag: term
#, no-c-format
msgid "--binary"
msgstr "--binary"

#. Tag: para
#, no-c-format
msgid "builds the package as a binary RPM package rather than a source RPM package."
msgstr "construit le paquet en tant que paquet RPM binaire au lieu de paquet source RPM."

#. Tag: term
#, no-c-format
msgid "publican print_banned"
msgstr "publican print_banned"

#. Tag: para
#, no-c-format
msgid "prints a list of DocBook tags banned by <application>Publican</application>."
msgstr "affiche une liste des balises DocBook refusées par <application>Publican</application>."

#. Tag: term
#, no-c-format
msgid "publican print_known"
msgstr "publican print_known"

#. Tag: para
#, no-c-format
msgid "prints a list of DocBook tags supported by <application>Publican</application>."
msgstr "affiche une liste des balises DocBook prises en charge par <application>Publican</application>."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican print_tree</command>"
msgstr "<command>publican print_tree</command>"

#. Tag: para
#, no-c-format
msgid "displays a tree of the XML files included in a document."
msgstr "affiche l'arborescence des fichiers XML faisant partie d'un document."

#. Tag: term
#, no-c-format
msgid "publican print_unused"
msgstr "publican print_unused"

#. Tag: para
#, no-c-format
msgid "prints a list of the XML files <emphasis>not</emphasis> included with the <sgmltag>&lt;xi:include&gt;</sgmltag> tag in a book, article, or set."
msgstr "affiche une liste des fichiers XML <emphasis>non</emphasis> inclus avec la balise <sgmltag>&lt;xi:include&gt;</sgmltag> dans un ouvrage, un article ou un ensemble."

#. Tag: term
#, no-c-format
msgid "publican print_unused_images"
msgstr "publican print_unused_images "

#. Tag: para
#, no-c-format
msgid "prints a list of the image files <emphasis>not</emphasis> referenced by an <sgmltag>&lt;imagedata&gt;</sgmltag> tag in a book, article, or set."
msgstr "affiche une liste des fichiers d'images <emphasis>non</emphasis> référencés par la balise <sgmltag>&lt;imagedata&gt;</sgmltag> dans un ouvrage, un article ou un ensemble."

#. Tag: term
#, no-c-format
msgid "publican remove_book"
msgstr "publican remove_book"

#. Tag: para
#, no-c-format
msgid "removes a document from a documentation website."
msgstr "supprime un document d'un site Web de documentation."

#. Tag: para
#, no-c-format
msgid "the language of the document to remove (mandatory)."
msgstr "la langue du document à supprimer (obligatoire)."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican rename</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "renames a document. Options:"
msgstr "renomme un document. Options :"

#. Tag: para
#, no-c-format
msgid "the new title for the document."
msgstr "le nouveau titre du document."

#. Tag: para
#, no-c-format
msgid "the new product to which the document applies."
msgstr "le nouveau produit sur lequel le document porte."

#. Tag: para
#, no-c-format
msgid "the new product version to which the document applies."
msgstr "la nouvelle version du produit sur lequel le document porte."

#. Tag: term
#, no-c-format
msgid "publican site_stats --site_config=<replaceable>name_of_site_config_file</replaceable>"
msgstr "publican site_stats --site_config=<replaceable>nom_fichier_configuration_du_site</replaceable>"

#. Tag: para
#, no-c-format
msgid "generates a site report for a documentation website. Option:"
msgstr "crée un rapport de site pour un site Web de documentation. Option :"

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican update_pot</command>"
msgstr "<command>publican update_pot</command>"

#. Tag: para
#, no-c-format
msgid "updates the POT files in a document."
msgstr "met à jour les fichiers POT d'un document."

#. Tag: term
#, no-c-format
msgid "<prompt>$</prompt> <command>publican update_po</command>"
msgstr "<command>publican update_po</command>"

#. Tag: para
#, no-c-format
msgid "updates the PO files in a document."
msgstr "met à jour les fichiers PO d'un document."

#. Tag: para
#, no-c-format
msgid "comma-separated list of languages to update, or &lsquo;all&rsquo; to update all (mandatory)."
msgstr "liste, avec virgule comme séparateur, des langues à mettre à jour, ou bien, &lsquo;all&rsquo; pour toutes (obligatoire)."

#. Tag: term
#, no-c-format
msgid "--msgmerge"
msgstr "--msgmerge"

#. Tag: para
#, no-c-format
msgid "use gettext's msgmerge for POT/PO merging."
msgstr "utilise msgmerge de gettext pour fusionner POT/PO."

#. Tag: term
#, no-c-format
msgid "publican update_site --site_config=<replaceable>name_of_site_config_file</replaceable>.cfg"
msgstr "publican update_site --site_config=<replaceable>nom_fichier_configuration_du_site</replaceable>.cfg"

#. Tag: para
#, no-c-format
msgid "regenerates the templated content of a documentation website. Option:"
msgstr "régénère le contenu modélisé d'un site Web de documentation. Option :"

#. Tag: title
#, no-c-format
msgid "Internal commands"
msgstr "Commandes internes"

#. Tag: para
#, no-c-format
msgid "<application>Publican</application> uses the commands documented in this section internally. There is normally no need to run them manually."
msgstr "<application>Publican</application> utilise en interne les commandes documentées dans ce paragraphe. Normalement, il n'est pas nécessaire de les exécuter manuellement."

#. Tag: term
#, no-c-format
msgid "publican update_db --add"
msgstr "publican update_db --add"

#. Tag: para
#, no-c-format
msgid "Adds entries to the database of a <application>Publican</application>-generated website, with the following options:"
msgstr "ajoute des entrées dans la base de données du site Web créé par <application>Publican</application>, avec les options :"

#. Tag: para
#, no-c-format
msgid "name of the site configuration file."
msgstr "nom du fichier de configuration du site."

#. Tag: para
#, no-c-format
msgid "the language in which the document is published."
msgstr "la langue de publication du document."

#. Tag: term
#, no-c-format
msgid "--formats"
msgstr "--formats"

#. Tag: para
#, no-c-format
msgid "a comma-separated list of the formats in which the document is published, for example, <literal>pdf,html-single</literal>"
msgstr "une liste, avec la virgule comme séparateur, des formats dans lesquels le document est publié, par exemple, <literal>pdf,html-single</literal>"

#. Tag: para
#, no-c-format
msgid "the title of the document."
msgstr "l'intitulé du document."

#. Tag: term
#, no-c-format
msgid "--name_label"
msgstr "--name_label"

#. Tag: para
#, no-c-format
msgid "the title of the document, as it should appear in the site's tables of contents."
msgstr "le titre du document, tel qu'il apparaît dans les tables des matières du site."

#. Tag: para
#, no-c-format
msgid "the product that the document describes."
msgstr "le produit que le document décrit."

#. Tag: term
#, no-c-format
msgid "--product_label"
msgstr "--product_label"

#. Tag: para
#, no-c-format
msgid "the product that the document describes, as it should appear in the site's tables of contents."
msgstr "le produit que le document décrit, tel qu'il apparaît dans la table des matières du site."

#. Tag: para
#, no-c-format
msgid "the version of the product that the document describes."
msgstr "la version du produit que le document décrit."

#. Tag: term
#, no-c-format
msgid "--version_label"
msgstr "--version_label"

#. Tag: para
#, no-c-format
msgid "the version of the product that the document describes, as it should appear in the site's tables of contents."
msgstr "la version du produit que le document décrit, tel qu'il apparaît dans la table des matières du site."

#. Tag: term
#, no-c-format
msgid "--subtitle"
msgstr "--subtitle"

#. Tag: para
#, no-c-format
msgid "the subtitle of the document."
msgstr "le sous-titre du document."

#. Tag: term
#, no-c-format
msgid "--abstract"
msgstr "--abstract"

#. Tag: para
#, no-c-format
msgid "the abstract of the document."
msgstr "le résumé du document."

#. Tag: para
#, no-c-format
msgid "For example:"
msgstr "Par exemple :"

#. Tag: programlisting
#, no-c-format
msgid ""
"publican update_db --add --lang en-US --formats html,pdf --name Foo \\\n"
"--name_label \"foo is good\" --version 0.1 --version_label UNUSED \\\n"
"--product Bar --product_label \"To the bar\" \\\n"
"--subtitle \"A guide to Bar Foo\" \\\n"
"--abstract \"There once was a Foo from Bar ...\" \\\n"
"--site_config /usr/share/bar/foo.cfg"
msgstr ""
"publican update_db --add --lang en-US --formats html,pdf --name Foo \\\n"
"--name_label \"foo is good\" --version 0.1 --version_label UNUSED \\\n"
"--product Bar --product_label \"To the bar\" \\\n"
"--subtitle \"A guide to Bar Foo\" \\\n"
"--abstract \"There once was a Foo from Bar ...\" \\\n"
"--site_config /usr/share/bar/foo.cfg"

#. Tag: term
#, no-c-format
msgid "publican update_db --del"
msgstr "publican update_db --del"

#. Tag: para
#, no-c-format
msgid "removes entries from the database of a <application>Publican</application>-generated website, with the following options:"
msgstr "supprime des entrées de la base de données du site Web créé par <application>Publican</application>, avec les options :"

#. Tag: programlisting
#, no-c-format
msgid "publican update_db --del --lang en-US --name Foo --version 0.1 --product Bar \\\n"
"--site_config /usr/share/bar/foo.cfg"
msgstr "publican update_db --del --lang en-US --name Foo --version 0.1 --product Bar \\\n"
"--site_config /usr/share/bar/foo.cfg"

